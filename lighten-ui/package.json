{
  "name": "lighten-ui",
  "version": "0.1.0",
  "description": "The lighten UI",
  "main": "index.js",
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "babel-node bin/compile",
    "eslint": "eslint",
    "lint": "eslint . --ext .js,.jsx .",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "esw . -w --ext .js,.jsx",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "dev:debug-window": "npm run dev -- --debug-window",
    "dev:no-debug": "npm run dev -- --no-debug",
    "test": "better-npm-run test",
    "test:debug": "npm run test -- --tests-debug",
    "test:no-coverage": "npm run test -- --no-coverage",
    "test:no-coverage:test-debug": "npm run test -- --no-coverage --tests-debug",
    "tdd": "npm run test -- --watch",
    "tdd:no-coverage": "npm run test -- --watch --no-coverage",
    "build": "better-npm-run build"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon --exec babel-node bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "build": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "babel-node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https@github.com:vbud/lighten-ui.git"
  },
  "author": "Valjean Clark <valjeanbud@gmail.com>",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^0.9.0",
    "better-npm-run": "0.0.6",
    "classnames": "^2.2.3",
    "compass-mixins": "^0.12.7",
    "debug": "^2.2.0",
    "history": "^2.0.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^3.0.0",
    "redux": "^3.0.0",
    "redux-actions": "^0.9.0",
    "redux-thunk": "^1.0.0",
    "superagent": "^1.8.3",
    "url": "^0.11.0",
    "yargs": "^3.18.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.3.17",
    "babel-eslint": "^5.0.0-beta10",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "css-loader": "^0.23.0",
    "cssnano": "^3.3.2",
    "es6-promise": "^3.0.2",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-config-standard-react": "^1.2.1",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.14.0",
    "eslint-plugin-standard": "^1.3.1",
    "eslint-watch": "^2.1.11",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.8.4",
    "fs-extra": "^0.26.3",
    "html-webpack-plugin": "^2.8.1",
    "imports-loader": "^0.6.5",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "karma": "0.13.19",
    "karma-coverage": "^0.5.0",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.23",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.2.5",
    "node-sass": "^3.4.2",
    "nodemon": "^1.8.1",
    "phantomjs-polyfill": "0.0.1",
    "phantomjs-prebuilt": "^2.0.0",
    "postcss-loader": "^0.8.0",
    "pretender": "^0.12.0",
    "react-addons-test-utils": "^0.14.0",
    "redux-devtools": "^3.0.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "redux-mock-store": "0.0.6",
    "sass-loader": "^3.0.0",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.1",
    "xhr-interceptor": "^1.3.0"
  }
}
